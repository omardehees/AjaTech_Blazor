@page "/_Host"
@namespace AjaTech_Blazor.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        @*--------------------------------------- dropDownList Table Style--------------------------------------------*@ .head1, .item1 {
            display: table;
            width: 100%;
            margin: auto;
        }

        .head1 {
            height: 40px;
            font-size: 15px;
            font-weight: 600;
        }

        .name1, .city1 {
            display: table-cell;
            vertical-align: middle;
            width: 30%;
        }

        .head1 .name1 {
            text-indent: 16px;
        }

        .head1 .city1 {
            text-indent: 10px;
        }

        @*------------------------------- / dropDownList Table Style--------------------------------------------------------*@
        .e-print-icon::before {
            content: '\e813';
            font-size: x-large;
            color: #000000
        }
        .e-settings-icon::before {
            content: '\e679';
            font-size: x-large;
            color: wheat
        }
        .e-LogOut-icon::before {
            content: '\e7e9';
            font-size: x-large;
            color: white
        }

        .control_wrapper {
            width: 350px;
            margin: 0 auto;
            /*padding-top: 70px;*/
        }
        /*Toast Style   */
        @@font-face {
            font-family: 'Toast_icons';
            src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
            font-weight: normal;
            font-style: normal;
        }

        .toast-icons {
            font-family: 'Toast_icons' !important;
            speak: none;
            font-size: 55px;
            font-style: normal;
            font-weight: normal;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

            .toast-icons.e-error::before {
                content: "\e700";
            }

            .toast-icons.e-success::before {
                content: "\e701";
            }

        .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }


        #Failed_Id_SelectRow .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Failed_Id_Add .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Failed_Id_Delete .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Failed_Id_Permission .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Failed_Id_Reload .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Failed_Id_Add_Empty .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }

        #Success_Id .e-toast-icon.e-icons {
            height: auto;
            font-size: 30px;
        }
        /*Toast Style*/
    </style>

    <!-- Get Uaser Angent info -->
    <script>
        window.getUserAgent = () => {
            return navigator.userAgent;
        };
    </script>
    <!-- Google Tag Manager -->
    <script>
        (function (w, d, s, l, i) {
